{
    "name": "workshop-orange/lagoon-api-php-cli",
    "description": "A Workshop Orange opinionated Lagoon API PHP cli that serves as a reference for workshop-orange/lagoon-api-php-client.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "/lagoon-api-php-client/"
        }
    ],
    "authors": [
        {
            "name": "Bryan Gruneberg",
            "email": "hello@bryangruneberg.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/log": "^9.0",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/termwind": "^1.3",
        "workshop-orange/lagoon-api-php-client": "@dev"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["lagoon-php"]
}
